package Demo;

import java.util.ArrayList;
import java.util.Iterator;

public class Q_50_RetrieveArrayLIst {

	public static void main(String[] args) {

		ArrayList list = new ArrayList();
		list.add("Bhavesh");// add element in 0 index
		list.add(10);// add element in 1 index
		list.add("java");// add element in 2 index
		list.add('P');// add element in 3 index
		list.add(4.133);// add element in 4 index
		list.add(true);// add element in 5 index
		list.add(54321);// add element in 6 index
		System.out.println(list);// print all element
		list.add(1, "HTML");// add element in 1 index
		list.add(3, 95);// add element in 3 index
		System.out.println(list);// print element
		
		System.out.println("\n "+list.get(3));
		
		
		
		Iterator itr = list.iterator();
		while (itr.hasNext()) {
			System.out.println(itr.next());

		}
		
	
	}

}
